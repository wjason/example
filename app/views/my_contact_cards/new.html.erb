<%= form_with(model: @my_contact_card ) do |form| %>
  <br>
  <br>
  <%= form.label :avatar %>
  <%= form.file_field :avatar %>
  <br>
  <br>
  <%= form.label :title %>
  <%= form.text_field :title %>
  <br>
  <br>
  <%= form.label :QRcode %>
  <%= form.file_field :QRcode %>
  <br>
  <br>
  <%= form.label :cdescription %>
  <%= form.text_area :cdescription %>
  <br>
  <br>
  <%= form.label :ctitle %>
  <%= form.text_field :ctitle %>
  <br>
  <br>
  <%= form.label :cpic %>
  <%= form.file_field :cpic %>
  <br>
  <br>
  <%= form.submit '提交' %>
<% end %>
